if(storage){var eventMethod=window.addEventListener?"addEventListener":"attachEvent";var eventer=window[eventMethod];var messageEvent=eventMethod=="attachEvent"?"onmessage":"message";eventer(messageEvent,function(message){if(message.origin.indexOf(_webhelper_source.BRAND.toLowerCase())==-1){return;}
